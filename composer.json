{
  "name": "lookyman/nette-proxy",
  "description": "Integration of Proxy Manager into Nette Framework",
  "keywords": ["Nette", "proxy", "lazy"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lukáš Unger",
      "email": "looky.msc@gmail.com",
      "homepage": "https://lookyman.net"
    }
  ],
  "require": {
    "ocramius/proxy-manager": "^2.0",
    "nette/di": "^3.0",
    "nette/php-generator": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "nette/bootstrap": "^2.4",
    "kdyby/console": "^2.6"
  },
  "suggest": {
    "kdyby/console": "For pre-generating proxies via a command"
  },
  "autoload": {
    "psr-4": {
      "Lookyman\\Nette\\Proxy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lookyman\\Nette\\Proxy\\Tests\\": "tests/"
    }
  }
}
